<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="ncover.settings" type="NCover.SettingsSectionHandler, NCover" />
  </configSections>
  <system.web>
    <caching>
      <!--<cache privateBytesLimit="1048576"/>-->
      <cache percentagePhysicalMemoryUsedLimit="4" />
    </caching>
  </system.web>
  <startup>
    <supportedRuntime version="v2.0.50727" />
  </startup>
  <!-- The following assemblies are automatically added to the module exclusion rules at runtime as a convenience.
       You can add or remove patterns from this list to change the default behavior.  -->
  <ncover.settings>
    <disableLocalCollection>false</disableLocalCollection>
    <enableRequestCache>true</enableRequestCache>
    <httpScheme>http</httpScheme>
    <httpServerPort>11235</httpServerPort>
    <analysisConnectionTimeout>90</analysisConnectionTimeout>
    <leaseTimeInDays>10</leaseTimeInDays>
    <sku>NC-DSK</sku>
    <!-- <sku>NC-DSK</sku> -->
    <!-- <sku>NC-COLL</sku> -->
    <!-- <sku>NC-CCSVR</sku> -->
    <updatesDownload>true</updatesDownload>
    <updatesChannel>
    </updatesChannel>
    <collectionTimeout>
    </collectionTimeout>
    <systemAssemblies>
      <assembly>accessibility.dll</assembly>
      <assembly>sort.*nlp</assembly>
      <assembly>msvc[mrp].*</assembly>
      <assembly>mscorcfg.*</assembly>
      <assembly>mscorlib.*</assembly>
      <assembly>sysglobl.*</assembly>
      <assembly>CppCodeProvider.*</assembly>
      <assembly>Microsoft.*</assembly>
      <assembly>PresentationBuildTasks.*</assembly>
      <assembly>PresentationCore.*</assembly>
      <assembly>PresentationFramework.*</assembly>
      <assembly>ReachFramework.*</assembly>
      <assembly>System\..*</assembly>
      <assembly>VSLangProj.*</assembly>
      <assembly>VSWebSite\.Interop.*</assembly>
      <assembly>VSWebSite\.Interop90.*</assembly>
      <assembly>SMDiagnostics.*</assembly>
      <assembly>UIAutomationClient.*</assembly>
      <assembly>UIAutomationClientsideProviders.*</assembly>
      <assembly>UIAutomationProvider.*</assembly>
      <assembly>UIAutomationTypes.*</assembly>
      <assembly>WindowsBase.*</assembly>
      <assembly>WindowsFormsIntegration.*</assembly>
      <assembly>vstesthost.*</assembly>
      <assembly>ISymWrapper.*</assembly>
      <assembly>nant\..*</assembly>
      <assembly>nunit\..*</assembly>
      <assembly>nunit-.*</assembly>
      <assembly>pnunit-.*</assembly>
      <assembly>xunit.*</assembly>
      <assembly>MbUnit.*</assembly>
      <assembly>MSTest.*</assembly>
      <assembly>log4net</assembly>
      <assembly>QuickGraph.*</assembly>
      <assembly>WiseOwl\.Obfuscation.*</assembly>
      <assembly>Rhino\.Mocks.*</assembly>
      <assembly>Gallio.*</assembly>
      <assembly>Bolt.*</assembly>
    </systemAssemblies>
  </ncover.settings>
</configuration>